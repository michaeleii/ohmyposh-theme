{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"version": 2,
	"final_space": true,
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "session",
					"style": "plain",
					"foreground": "lightRed",
					"template": "Michael "
				},
				{
					"type": "path",
					"style": "plain",
					"foreground": "lightGreen",
					"template": "\ue5ff {{ .Path }} ",
					"properties": {
						"style": "folder"
					}
				},
				{
					"type": "git",
					"style": "plain",
					"foreground": "lightCyan",
					"template": "{{ .HEAD }}{{ if gt .StashCount 0 }}\uf692 {{ .StashCount }}{{ end }} ",
					"properties": {
						"branch_icon": "<lightCyan><b>\ue0a0 </b></>",
						"fetch_stash_count": true
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				{
					"type": "text",
					"style": "plain",
					"foreground": "darkGray",
					"template": "‚ùØ"
				}
			],
			"newline": true
		}
	]
}
